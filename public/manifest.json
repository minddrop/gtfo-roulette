{
  "manifest_version": 2,
  "name": "GTFO roulette",
  "description": "decide gtfo weapons and map at random",
  "version": "1.0",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
